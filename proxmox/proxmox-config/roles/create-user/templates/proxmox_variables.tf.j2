############################################################
# This variables values come from 'proxmox-credentials.tfvars' file.
# This file is automatically generated by 'proxmox-config' code.
############################################################
variable "proxmox_api_url" {
    description = "Proxmox API URL"
    type = string
    default = "{{ proxmox_api_url }}"
}
variable "proxmox_token_id" {
    description = "Proxmox User ID"
    type = string
    default = "{{ proxmox_token_id }}"
}
variable "proxmox_token_secret" {
    description = "Proxmox User Secret"
    type = string
    default = "{{ proxmox_token_secret }}"
}
variable "proxmox_vm_template_name" {
    description = "VM Template name in Proxmox"
    type = string
    default = "{{ vm_template_name }}"
}